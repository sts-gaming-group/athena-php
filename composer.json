{
  "name": "sts-gaming-group/athena",
  "version": "1.0.2",
  "description": "Security library that provides an additional layer of functionality over the composer audit, specifically tailored for managing ignored advisories.",
  "type": "library",
  "keywords": ["audit", "security"],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcin Worwa",
      "email": "marcin.worwa@sts.pl"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/console": ">=6.2"
  },
  "autoload": {
    "psr-4": {
      "Athena\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Athena\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "bin": [
    "bin/athena"
  ]
}
